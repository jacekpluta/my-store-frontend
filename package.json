    "@types/styled-components": "5.1.0",
    "apollo-boost": "0.1.28",
    "apollo-cache": "1.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-micro": "2.14.2",
