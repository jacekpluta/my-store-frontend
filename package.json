{
  "name": "my-store",
  "version": "0.1.0",
  "author": "",
  "description": "My Store",
  "license": "ISC",
  "scripts": {
    "codegen": "graphql-let",
    "dev": "yarn codegen && next -p 5000",
    "build": "yarn codegen && next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "@apollo/react-common": "3.1.4",
    "@apollo/react-components": "3.1.5",
    "@apollo/react-hooks": "3.1.5",
    "@types/nprogress": "0.2.0",
    "@types/styled-components": "5.1.0",
    "apollo-boost": "0.1.28",
    "apollo-cache": "1.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-micro": "2.14.2",
    "apollo-utilities": "1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-styled-components": "1.10.7",
    "date-fns": "^2.14.0",
    "downshift": "^2.2.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "lodash.debounce": "^4.0.8",
    "next": "^9.4.4",
    "next-with-apollo": "3.4.0",
    "nprogress": "0.2.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-adopt": "0.6.0",
    "react-apollo": "2.5.8",
    "react-dom": "16.13.1",
    "react-stripe-checkout": "2.6.3",
    "react-transition-group": "2.9.0",
    "styled-components": "5.1.1",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.1",
    "@graphql-codegen/plugin-helpers": "1.15.1",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-operations": "1.13.3",
    "@graphql-codegen/typescript-react-apollo": "1.13.3",
    "@graphql-codegen/typescript-resolvers": "1.15.1",
    "@types/node": "14.0.13",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "3.2.0",
    "casual": "1.6.2",
    "enzyme-to-json": "3.5.0",
    "graphql-let": "0.x",
    "graphql-tools": "4.0.8",
    "jest": "23.6.0",
    "jest-transform-graphql": "2.1.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "//": "babel config",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
