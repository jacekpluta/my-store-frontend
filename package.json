    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^23.3.1",
    "@types/node": "14.0.13",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "^1.10.7",
    "casual": "1.6.2",
