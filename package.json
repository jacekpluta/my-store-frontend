    "react-adopt": "0.6.0",
    "react-apollo": "2.5.8",
    "react-dom": "16.12.0",
    "react-is": "^16.13.1",
    "react-spinners": "^0.9.0",
    "react-stripe-checkout": "2.6.3",
    "react-transition-group": "2.9.0",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.1.3",
    "waait": "1.0.5"
